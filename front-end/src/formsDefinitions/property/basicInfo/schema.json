{
    "title": "Basic Information",
    "type": "object",
    "required": [
        "sellProperty",
        "addPropertyType",
        "rentalProperty",
        "label",
        "buildingType",
        "propertyType",
        "subPropertyType",
        "propertyOwnership"
    ],
    "properties": {
        "addPropertyType": {
            "title": "Property Type",
            "enum": [
                "new-property",
                "old-property"
            ],
            "enumNames": [
                "New Property",
                "Old Property"
            ],
            "default": "new-property"
        },
        "sellProperty": {
            "title": "Sells Property",
            "type": "string"
        },
        "rentalProperty": {
            "title": "Rental Property",
            "type": "string"
        },
        "label": {
            "title": "Label",
            "enumNames": [
                "Tower",
                "Wing",
                "Unit",
                "Type"
            ],
            "enum": [
                "tower",
                "wing",
                "unit",
                "type"
            ]
        },
        "buildingType": {
            "title": "Building Type",
            "enumNames": [
                "Single Unit",
                "Low Rise",
                "Mid Rise",
                "High Rise"
            ],
            "enum": [
                "single-unit",
                "low-rise",
                "mid-rise",
                "high-rise"
            ]
        },
        "propertyOwnership": {
            "title": "Property Ownership",
            "enumNames": [
                "Individual  Ownership",
                "Joint ownership",
                "Nomination Ownership",
                "Co-ownership",
                "Company",
                "Partnership Firm"
            ],
            "enum": [
                "individual-ownership",
                "joint-ownership",
                "nomination-ownership",
                "co-ownership",
                "company",
                "partnership-firm"
            ]
        },
        "propertyType": {
            "title": "Property Type",
            "enumNames": [
                "Residential",
                "Commercial",
                "Industrial",
                "Agricultural"
            ],
            "enum": [
                "residential",
                "commercial",
                "industrial",
                "agricultural"
            ],
            "default": "residential"
        },
        "subPropertyType": {
            "title": "Sub Property Type",
            "type": "string"
        }
    },
    "allOf": [
        {
            "if": {
                "properties": {
                    "addPropertyType": {
                        "enum": [
                            "new-property"
                        ]
                    }
                }
            },
            "then": {
                "properties": {
                    "sellProperty": {
                        "enumNames": [
                            "Unit Sells",
                            "Pree Lease Sell"
                        ],
                        "enum": [
                            "unit-sells",
                            "pree-lease-sells"
                        ]
                    },
                    "rentalProperty": {
                        "enumNames": [
                            "Rent",
                            "Pree Lease Rent",
                            "Lease"
                        ],
                        "enum": [
                            "rent",
                            "pree-lease-rent",
                            "lease"
                        ]
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "addPropertyType": {
                        "enum": [
                            "old-property"
                        ]
                    }
                }
            },
            "then": {
                "properties": {
                    "sellProperty": {
                        "enumNames": [
                            "Re Sells",
                            "Pree Lease Re Sells"
                        ],
                        "enum": [
                            "re-sells",
                            "pree-lease-re-sells"
                        ]
                    },
                    "rentalProperty": {
                        "enumNames": [
                            "Rent",
                            "Pree Lease Rent",
                            "Lease"
                        ],
                        "enum": [
                            "rent",
                            "pree-lease-rent",
                            "lease"
                        ]
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "propertyType": {
                        "enum": [
                            "residential"
                        ]
                    }
                }
            },
            "then": {
                "required": [
                    "roomTypes"
                ],
                "properties": {
                    "subPropertyType": {
                        "enum": [
                            "bungalow",
                            "villa",
                            "condominium",
                            "cottage",
                            "row-house",
                            "duplex-house",
                            "triplex-house",
                            "farm-house",
                            "stilt-house",
                            "tree-house",
                            "hut",
                            "eco-friendly-house",
                            "any-other"
                        ],
                        "enumNames": [
                            "Bungalow",
                            "Villa",
                            "Condominium",
                            "Cottage",
                            "Row House",
                            "Duplex House",
                            "Triplex House",
                            "Farm House",
                            "Stilt House",
                            "Tree House",
                            "Hut",
                            "Eco Friendly House",
                            "Other If Any"
                        ],
                        "default": "apartment"
                    },
                    "apartments": {
                        "title": "Apartments",
                        "enumNames": [
                            "Flat",
                            "Penthouse",
                            "Terrace Flat",
                            "Filament",
                            "Studio Apartment"
                        ],
                        "enum": [
                            "flat",
                            "penthouse",
                            "terrace-flat",
                            "filament",
                            "studio-apartment"
                        ]
                    },
                    "roomTypes": {
                        "title": "Rooms Type",
                        "enumNames": [
                            "2-BHK",
                            "3-BHK",
                            "3.5-BHK",
                            "4-BHK",
                            "4.5-BHK",
                            "5-BHK",
                            "6-BHK",
                            "3B_2HK",
                            "4B_2HK",
                            "5B_4HK",
                            "6B_2HK",
                            "7B_2HK"
                        ],
                        "enum": [
                            "2bhk",
                            "3-BHK",
                            "3.5-BHK",
                            "4-BHK",
                            "4.5-BHK",
                            "5-BHK",
                            "6-BHK",
                            "3B_2HK",
                            "4B_2HK",
                            "5B_4HK",
                            "6B_2HK",
                            "7B_2HK"
                        ]
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "propertyType": {
                        "enum": [
                            "commercial"
                        ]
                    }
                }
            },
            "then": {
                "properties": {
                    "typesOfHouse": {
                        "title": "Types of House",
                        "enumNames": [
                            "Complex",
                            "Plaza",
                            "Hub"
                        ],
                        "enum": [
                            "complex",
                            "plaza",
                            "hub"
                        ]
                    },
                    "subPropertyType": {
                        "enumNames": [
                            "Office",
                            "Shops",
                            "Showrooms",
                            "Commercial Open Plots",
                            "Commercial Land",
                            "Multifamily Use Land",
                            "Multifamily Use Plot",
                            "Retails Shops",
                            "Storage Warehouse",
                            "Hospital",
                            "Shopping Centre",
                            "Other If Any"
                        ],
                        "enum": [
                            "office",
                            "shops",
                            "showrooms",
                            "commercial-open-plots",
                            "commercial-land",
                            "multifamily-use-land",
                            "multifamily-use-plot",
                            "retails-shops",
                            "storage-warehouse",
                            "hospital",
                            "shopping-centre",
                            "any-other"
                        ],
                        "default": "office"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "propertyType": {
                        "enum": [
                            "industrial"
                        ]
                    }
                }
            },
            "then": {
                "properties": {
                    "subPropertyType": {
                        "enumNames": [
                            "Ind. Office",
                            "Industrial Open Plots",
                            "Industrial Land",
                            "Special Purpose Land(Economics)",
                            "Mixed Used Land",
                            "Mixed Use Plot",
                            "Multifamily Land",
                            "Warehouse",
                            "Factories",
                            "Industrial Shed",
                            "Other If Any"
                        ],
                        "enum": [
                            "ind-office",
                            "industrial-open-plots",
                            "industrial-land",
                            "special-purpose-land",
                            "mixed-used-land",
                            "mixed-use-plot",
                            "multifamily-land",
                            "warehouse",
                            "factories",
                            "industrial-shed",
                            "any-other"
                        ],
                        "default": "ind-office"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "propertyType": {
                        "enum": [
                            "agricultural"
                        ]
                    }
                }
            },
            "then": {
                "properties": {
                    "subPropertyType": {
                        "enumNames": [
                            "Cropped Land",
                            "New Condition Land",
                            "Old Conditions Land",
                            "Dry Land",
                            "Non-agricultural land",
                            "Barren & wastelands",
                            "Permanent Pastures and grazing Land",
                            "Miscellaneous tree crops & groves land",
                            "Culturable waste land",
                            "Other If Any"
                        ],
                        "enum": [
                            "cropped-land",
                            "new-condition-land",
                            "old-conditions-land",
                            "dry-land",
                            "non-agricultural-land",
                            "barren-wastelands",
                            "permanent-pastures-and-grazing-land",
                            "miscellaneous-tree-crops-groves-land",
                            "culturable-waste-land",
                            "any-other"
                        ],
                        "default": "cropped-land"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "subPropertyType": {
                        "enum": [
                            "any-other"
                        ]
                    }
                }
            },
            "then": {
                "properties": {
                    "otherProperty": {
                        "type": "string",
                        "title": "Add Other Property"
                    }
                }
            }
        }
    ]
}