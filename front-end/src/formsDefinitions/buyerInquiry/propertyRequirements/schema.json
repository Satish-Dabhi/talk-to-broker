{
    "title": "Property Requirements",
    "type": "object",
    "required": [
        "timeLineToBuy",
        "timeLine",
        "utilityPurpose",
        "buildingType"
    ],
    "properties": {
        "propertyType": {
            "enum": [
                "residential",
                "commercial",
                "industrial",
                "agricultural"
            ],
            "hidden": true
        },
        "timeLineToBuy": {
            "title": "Time Line To Buy",
            "enum": [
                "months",
                "years"
            ],
            "enumNames": [
                "Months",
                "Years"
            ],
            "default": "months"
        },
        "timeLine": {
            "title": "Time Line",
            "type": "string"
        },
        "utilityPurpose": {
            "title": "Utility Purpose",
            "enum": [
                "personal",
                "investment",
                "for-others"
            ],
            "enumNames": [
                "Personal",
                "Investment",
                "For Others"
            ]
        },
        "buildingType": {
            "title": "Building Type",
            "enum": [
                "single-unit",
                "low-rise",
                "mid-rise",
                "high-rise"
            ],
            "enumNames": [
                "Single Unit",
                "Low Rise",
                "Mid Rise",
                "High Rise"
            ]
        },
        "subPropertyType": {
            "title": "Sub Property Type",
            "type": "string"
        }
    },
    "allOf": [
        {
            "if": {
                "properties": {
                    "timeLineToBuy": {
                        "enum": [
                            "months"
                        ]
                    }
                }
            },
            "then": {
                "properties": {
                    "timeLine": {
                        "enumNames": [
                            "1 Month",
                            "2 Months",
                            "3 Months",
                            "4 Months",
                            "5 Months",
                            "6 Months",
                            "7 Months",
                            "8 Months",
                            "9 Months",
                            "10 Months",
                            "11 Months"
                        ],
                        "enum": [
                            "1-month",
                            "2-months",
                            "3-months",
                            "4-months",
                            "5-months",
                            "6-months",
                            "7-months",
                            "8-months",
                            "9-months",
                            "10-months",
                            "11-months"
                        ]
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "timeLineToBuy": {
                        "enum": [
                            "years"
                        ]
                    }
                }
            },
            "then": {
                "properties": {
                    "timeLine": {
                        "enumNames": [
                            "1 Year",
                            "2 Years",
                            "3 Years",
                            "4 Years",
                            "5 Years"
                        ],
                        "enum": [
                            "1-year",
                            "2-years",
                            "3-years",
                            "4-years",
                            "5-years"
                        ]
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "propertyType": {
                        "enum": [
                            "residential"
                        ]
                    }
                }
            },
            "then": {
                "required": [
                    "roomTypes",
                    "apartments",
                    "size",
                    "budget",
                    "floorSelection"
                ],
                "properties": {
                    "subPropertyType": {
                        "enum": [
                            "bungalow",
                            "villa",
                            "condominium",
                            "cottage",
                            "row-house",
                            "duplex-house",
                            "triplex-house",
                            "farm-house",
                            "stilt-house",
                            "tree-house",
                            "hut",
                            "eco-friendly-house",
                            "any-other"
                        ],
                        "enumNames": [
                            "Bungalow",
                            "Villa",
                            "Condominium",
                            "Cottage",
                            "Row House",
                            "Duplex House",
                            "Triplex House",
                            "Farm House",
                            "Stilt House",
                            "Tree House",
                            "Hut",
                            "Eco Friendly House",
                            "Other If Any"
                        ],
                        "default": "apartment"
                    },
                    "apartments": {
                        "title": "Apartments",
                        "enumNames": [
                            "Flat",
                            "Penthouse",
                            "Terrace Flat",
                            "Filament",
                            "Studio Apartment"
                        ],
                        "enum": [
                            "flat",
                            "penthouse",
                            "terrace-flat",
                            "filament",
                            "studio-apartment"
                        ]
                    },
                    "roomTypes": {
                        "title": "Rooms Type",
                        "enumNames": [
                            "2-BHK",
                            "3-BHK",
                            "3.5-BHK",
                            "4-BHK",
                            "4.5-BHK",
                            "5-BHK",
                            "6-BHK",
                            "3B_2HK",
                            "4B_2HK",
                            "5B_4HK",
                            "6B_2HK",
                            "7B_2HK"
                        ],
                        "enum": [
                            "2bhk",
                            "3-BHK",
                            "3.5-BHK",
                            "4-BHK",
                            "4.5-BHK",
                            "5-BHK",
                            "6-BHK",
                            "3B_2HK",
                            "4B_2HK",
                            "5B_4HK",
                            "6B_2HK",
                            "7B_2HK"
                        ]
                    },
                    "size": {
                        "title": "Size",
                        "enumNames": [
                            "Plot Size",
                            "Carpet Size"
                        ],
                        "enum": [
                            "plot-size",
                            "carpet-size"
                        ]
                    },
                    "budget": {
                        "title": "Budget",
                        "type": "number"
                    },
                    "floorSelection": {
                        "title": "Floor Selection",
                        "type": "number"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "propertyType": {
                        "enum": [
                            "commercial"
                        ]
                    }
                }
            },
            "then": {
                "required": [
                    "typesOfHouse",
                    "size",
                    "budget",
                    "floorSelection"
                ],
                "properties": {
                    "typesOfHouse": {
                        "title": "Types of House",
                        "enumNames": [
                            "Complex",
                            "Plaza",
                            "Hub"
                        ],
                        "enum": [
                            "complex",
                            "plaza",
                            "hub"
                        ]
                    },
                    "subPropertyType": {
                        "enumNames": [
                            "Office",
                            "Shops",
                            "Showrooms",
                            "Commercial Open Plots",
                            "Commercial Land",
                            "Multifamily Use Land",
                            "Multifamily Use Plot",
                            "Retails Shops",
                            "Storage Warehouse",
                            "Hospital",
                            "Shopping Centre",
                            "Other If Any"
                        ],
                        "enum": [
                            "office",
                            "shops",
                            "showrooms",
                            "commercial-open-plots",
                            "commercial-land",
                            "multifamily-use-land",
                            "multifamily-use-plot",
                            "retails-shops",
                            "storage-warehouse",
                            "hospital",
                            "shopping-centre",
                            "any-other"
                        ],
                        "default": "office"
                    },
                    "size": {
                        "title": "Size",
                        "enumNames": [
                            "Plot Size",
                            "Carpet Size"
                        ],
                        "enum": [
                            "plot-size",
                            "carpet-size"
                        ]
                    },
                    "budget": {
                        "title": "Budget",
                        "type": "number"
                    },
                    "floorSelection": {
                        "title": "Floor Selection",
                        "type": "number"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "propertyType": {
                        "enum": [
                            "industrial"
                        ]
                    }
                }
            },
            "then": {
                "required": [
                    "size",
                    "budget",
                    "floorSelection"
                ],
                "properties": {
                    "subPropertyType": {
                        "enumNames": [
                            "Ind. Office",
                            "Industrial Open Plots",
                            "Industrial Land",
                            "Special Purpose Land(Economics)",
                            "Mixed Used Land",
                            "Mixed Use Plot",
                            "Multifamily Land",
                            "Warehouse",
                            "Factories",
                            "Industrial Shed",
                            "Other If Any"
                        ],
                        "enum": [
                            "ind-office",
                            "industrial-open-plots",
                            "industrial-land",
                            "special-purpose-land",
                            "mixed-used-land",
                            "mixed-use-plot",
                            "multifamily-land",
                            "warehouse",
                            "factories",
                            "industrial-shed",
                            "any-other"
                        ],
                        "default": "ind-office"
                    },
                    "size": {
                        "title": "Size",
                        "enumNames": [
                            "Plot Size",
                            "Carpet Size"
                        ],
                        "enum": [
                            "plot-size",
                            "carpet-size"
                        ]
                    },
                    "budget": {
                        "title": "Budget",
                        "type": "number"
                    },
                    "floorSelection": {
                        "title": "Floor Selection",
                        "type": "number"
                    }
                }
            }
        },
        {
            "if": {
                "properties": {
                    "propertyType": {
                        "enum": [
                            "agricultural"
                        ]
                    }
                }
            },
            "then": {
                "required": [
                    "size",
                    "budget",
                    "area"
                ],
                "properties": {
                    "subPropertyType": {
                        "enumNames": [
                            "Cropped Land",
                            "New Condition Land",
                            "Old Conditions Land",
                            "Dry Land",
                            "Non-agricultural land",
                            "Barren & wastelands",
                            "Permanent Pastures and grazing Land",
                            "Miscellaneous tree crops & groves land",
                            "Culturable waste land",
                            "Other If Any"
                        ],
                        "enum": [
                            "cropped-land",
                            "new-condition-land",
                            "old-conditions-land",
                            "dry-land",
                            "non-agricultural-land",
                            "barren-wastelands",
                            "permanent-pastures-and-grazing-land",
                            "miscellaneous-tree-crops-groves-land",
                            "culturable-waste-land",
                            "any-other"
                        ],
                        "default": "cropped-land"
                    },
                    "size": {
                        "title": "Size",
                        "enum": [
                            "sq-ft",
                            "sq-mtr",
                            "acres",
                            "hector",
                            "yard",
                            "bigha"
                        ],
                        "enumNames": [
                            "Sq. ft.",
                            "Sq. mtr.",
                            "Acres",
                            "Hector",
                            "Yard",
                            "Bigha"
                        ]
                    },
                    "budget": {
                        "title": "Budget",
                        "type": "number"
                    },
                    "area": {
                        "title": "Area Required",
                        "type": "number"
                    }
                }
            }
        }
    ]
}